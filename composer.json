{
  "name": "board3r/rakuten-sdk",
  "description": "A PHP SDK to wrap the Rakuten's France API. This enables you to develop a fast, flexible and custom integration for your existing e-commerce solution.",
  "keywords": [
    "sdk",
    "rakuten",
    "api"
  ],
  "type": "library",
  "version": "v1.0.0-alpha",
  "license": "GPL-3.0-or-later",
  "authors": [
    {
      "name": "Maxime DANJOU",
      "email": "maxime.danjou31@gmail.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "RakutenSDK\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "RakutenSDK\\Test\\": "test/"
    }
  },
  "require": {
    "php": "^8.1",
    "guzzlehttp/guzzle": "~7.0",
    "guzzlehttp/psr7": "~2.0",
    "ext-mbstring": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "~9.5.0"
  }
}